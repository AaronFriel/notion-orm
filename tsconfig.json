{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "es2016",                                 
    "lib": ["es6"],                                      
    "module": "commonjs",                                
    "rootDir": "src",                                    
    "resolveJsonModule": true,                           
    "allowJs": true,                                     
    "outDir": "build",                                    
    "esModuleInterop": true,                             
    "strict": true,      
    "baseUrl": "src",                                
    "noImplicitAny": true,
    "paths": {
      "@notion-database/*":["NotionActions/DatabaseTypes/*"]
    }                               
  },
  "include": ["src/**/*", "src/NotionActions/DatabaseTypes"]
}


